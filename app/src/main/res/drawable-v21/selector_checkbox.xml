<?xml version="1.0" encoding="utf-8"?>
<animated-selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/check" android:drawable="@drawable/v_checkbox_check" android:state_checked="true"/>
    <item android:id="@+id/uncheck" android:drawable="@drawable/v_checkbox_uncheck"/>
    <transition
        android:drawable="@drawable/anim_checked_unchecked"
        android:fromId="@id/check"
        android:toId="@id/uncheck"/>
    <transition
        android:drawable="@drawable/anim_unchecked_checked"
        android:fromId="@id/uncheck"
        android:toId="@id/check"/>
</animated-selector>